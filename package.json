{
  "name": "multicoder",
  "version": "1.0.0",
  "description": "Unified authentication and profile management for multi-provider AI development - supporting Anthropic Claude, Google Gemini, OpenAI/Codex, Amazon Q, and more.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "coders": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "readme_zh.md",
    "LICENSE"
  ],
  "author": "Multicoder Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ljyou001/multicoder-auth.git"
  },
  "bugs": {
    "url": "https://github.com/ljyou001/multicoder-auth/issues"
  },
  "homepage": "https://github.com/ljyou001/multicoder-auth#readme",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth/*": "./dist/auth/*.js",
    "./profile/*": "./dist/profile/*.js",
    "./core/*": "./dist/core/*.js",
    "./examples/*": "./examples/*",
    "./scripts/*": "./scripts/*",
    "./tests/*": "./tests/*"
  },
  "typesVersions": {
    "*": {
      "auth/*": [
        "dist/auth/*.d.ts"
      ],
      "profile/*": [
        "dist/profile/*.d.ts"
      ],
      "core/*": [
        "dist/core/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "npm run build && node --test tests/unit",
    "test:quick": "npm run build && node scripts/test-installed.js",
    "test:codex": "npm run build && node --test tests/unit/providers/codex.test.js",
    "test:codex-credentials": "npm run build && node scripts/test-codex-credentials.js",
    "test:gemini": "npm run build && node --test tests/unit/providers/gemini.test.js",
    "test:claude": "npm run build && node --test tests/unit/providers/claude.test.js"
  },
  "keywords": [
    "authentication",
    "auth",
    "profile",
    "profiles",
    "credential-management",
    "multicoder",
    "ai",
    "anthropic",
    "claude",
    "gemini",
    "openai",
    "codex",
    "amazon-q",
    "oauth",
    "api-key",
    "multi-provider",
    "cli"
  ],
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.24",
    "rimraf": "^5.0.7",
    "typescript": "^5.3.3"
  }
}
